        <class-b>com.co.microservices.base.domain.InfoGeneral.request.token.TokenRequest</class-b>
        <field>
            <a>companyId</a>
            <b>companyId</b>
        </field>
        <field>
            <a>password</a>
            <b>password</b>
        </field>
        <field>
            <a>userName</a>
            <b>userName</b>
        </field>
    </mapping>
